#include<stdio.h>
#include<math.h>
int main ()
{   int i,num,sum=0;
printf("Enter number \n");
scanf("%d",&num);
for(i=1;i<=num;i++)
{
    sum=sum+pow(i,2);
}
printf("Sum of series\n");
for (i = 1; i <=num; i++)
{
    if(i!=num)
    {
        printf("%d^2+ ",i);
    }
    else
    {
        printf("%d^2 = %d\n",i,sum);
    }
}    
}
