#include<stdio.h>
int occurence(char ch[],char c){
    char *ptr=ch;
    int count=0;
    while (*ptr!='\0')
    {
        if(*ptr==c){
            count++;
        }
    ptr++;
    }
    return count;
}
int main(){
    char ch[]="github.com/Mayankagg44";
    int count= occurence(ch,'a');
    printf("Total occurence = %d",count);
return 0;
}
