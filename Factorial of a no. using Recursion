#include<stdio.h>
int factorial(int x); //Function Prototype
int main()
{   int a=9; //You can take input from the user as well using scanf()
    printf("The factoral of %d is :%d ",a,factorial(a)); //Function Calling
    return 0;      //Github.com/Mayankagg44
}
int factorial(int x){   //Function Definition
    printf("Calling factorial (%d)\n",x);
    if(x==1||x==0){
        return 1;
    }
    else
    {
        return x*factorial(x-1);
    }
    
}
