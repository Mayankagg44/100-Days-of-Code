#include<stdio.h>

int main(){
    int n = 23, prime=1;   //Change the value of n for different answer
    for(int i=2;i<n;i++){
        if (n%i==0 ){
            prime = 0;
            break;
        }
    }
    if (prime==0){
        printf("Your number is: %d\n",n);
        printf("This is not a prime number");
    }
    else{
        printf("Your number is: %d\n",n);
        printf("This is a prime number");
    }
    return 0;
}
