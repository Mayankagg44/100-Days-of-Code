#include<stdio.h>
#include<string.h>
struct employee
{
    int code;
    char name[10];
    float salary;
};

int main(){
    struct employee e;
    e.code=110;
    strcpy(e.name,"Mayank");        //e.name="Mayank"; wont work
    e.salary=100000;   
    printf("%d\n",e.code);
    printf("%s\n",e.name);
    printf("%.2f\n",e.salary);
    return 0;
}
